MILVUS_HOST=localhost
MILVUS_PORT=19530

# Milvus schema assumptions
MILVUS_VECTOR_FIELD=vector
MILVUS_ID_FIELD=id

# Collection names (edit to match your actual Milvus collections)
MILVUS_ORF_COLLECTION=orf_profiles
MILVUS_CRISPR_COLLECTION=crispr_profiles
MILVUS_COMPOUND_COLLECTION=compound_profiles

METADATA_BACKEND=duckdb
DUCKDB_PATH=./data/metadata.duckdb

SMOKE_QUERY_ID=demo_0
SMOKE_DATASET=orf
SMOKE_K=10

MILVUS_MODE=lite
MILVUS_LITE_PATH=./data/milvus_lite.db

# Or Postgres for future implementations:
# METADATA_BACKEND=postgres
# POSTGRES_DSN=postgresql+psycopg://user:pass@host:5432/dbname
